Host *
  ForwardAgent yes
  UseKeychain yes
  AddKeysToAgent yes

Host hub
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly yes

Host ee
  HostName gitee.com
  User git
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly yes

Host lab
  HostName gitlab.offerhk.com
  User git
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly yes

# dev(lxbird & hkok)
Host dev
  HostName 39.108.123.165
  User root
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  PubKeyAuthentication yes
  IdentityFile ~/.ssh/id_rsa

